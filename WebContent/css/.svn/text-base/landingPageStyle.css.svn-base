	background:url(../images/basicForm_inputRighttBg.png) left center no-repeat;
	}

.commentPanelRightBg{
	width:8px;
	height:46px;
	float:left;
	background:url(../images/comment_RightBg.png) left center no-repeat;
	}		
.inputTextfield{
	width:220px;
	height:20px;
	float:left;
	border:0px;
	outline:none;
	background:transparent;
	
	font-family:Arial;
	font-size:11px;
	font-weight:normal;
	color:#424242;
	text-align:left;
	}	
	
.inputjumptext{
	border:none;
	width:238px;
	background:transparent;
	margin-top:2px;
	outline:none;
	text-align:left;
	font-family:Arial;
	font-size:12px;
	font-weight:normal;
	color:#424242;
	}		
.dateHeadingText{
	font-family:Arial;
	font-size:12px;
	font-style:italic;
	font-weight:normal;
	color:#040404;
	text-align:left;
	margin-left:5%;
	/* padding-top:5px;
	 */width:95%;
	float:left;
}

.datePanel{
	width:405px;
	height:65px;
	float:left;
	border:1px solid #e7e7e7;
	
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	margin-left:15px;
	margin-top:4px;
	margin-bottom:5px;
	padding-top:5px;
	
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#f3f3f3'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#ffffff), to(#f3f3f3)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #ffffff,  #f3f3f3); /* for firefox 3.6+ */}	
	
	
.inputTextArea{
	width:235px;
	height:40px;
	margin-top:2px;
	float:left;
	border:0px;
	outline:none;
	background:transparent;
	
	font-family:Arial;
	font-size:11px;
	font-weight:normal;
	color:#424242;
	text-align:left;
	
	}		

.basicform_button{width:63px; height:22px; 
	background:url(../images/BasicForm_button.png) left center no-repeat; 
