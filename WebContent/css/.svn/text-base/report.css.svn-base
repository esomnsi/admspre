.table{
    border-collapse: collapse;
    width:100%;
    
}

.reportHeader{
    font-family: arial;
    font-size: 12px;
   	text-align:center;
    height:25px;
  	color: #000000;
	font-weight: bold;
	background-color: #efefef;
	border: 1px solid #D6D6D6;
}

.reportSubHeader{
    font-family: arial;
    font-size: 12px;
   	text-align:center;
    height:25px;
  	color: #000000;
	font-weight: bold;
	background-color: #efefef;
	border: 1px solid #D6D6D6;
}

.reportCell{
    border: 1px solid #D6D6D6;
    text-align: center;
    font-family: arial;
    font-size: 12px;
    
}

.reportHeaderLabel{
 	font-size: 13px;
	margin-top:5px;
	line-height:25px;
	padding-left:15px;
   	text-align:left;
    height:25px;
	color: #FFFFFF;
	font-family: Arial;
	
	background-image:-webkit-gradient(linear, left top, left bottom, from(#0c3d67), to(#275d92));
	background-image:-webkit-linear-gradient(#0c3d67, #275d92);
	background-image:-moz-linear-gradient(#0c3d67, #275d92);
	background-image:-ms-linear-gradient(#0c3d67, #275d92);
	background-image:-o-linear-gradient(#0c3d67, #275d92);
	background-image:linear-gradient(#0c3d67, #275d92);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#0c3d67', endColorstr='#275d92');
 }
 
 .reportCellLeft{
    text-align: left;
    margin-left: 10px;
    text-indent: 5px;
}


.exportlinks{
	margin-top:10px;
    font-family: arial;
    font-size: 12px;
   	text-align:right;
    height:25px;
  	color: #000000;
	font-weight: bold;
	
	
}

