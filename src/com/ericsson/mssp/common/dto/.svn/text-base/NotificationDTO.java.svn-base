package com.ericsson.mssp.common.dto;

public class NotificationDTO {
	private Integer solutionId=null;
	private Integer notificationType=null;
	private String prevApprover=null;
	private String nextApprover=null;
	private String solutionCreator=null;
	private String recipientEmailId=null;
	
	public Integer getSolutionId() {
		return solutionId;
	}
	public void setSolutionId(Integer solutionId) {
		this.solutionId = solutionId;
	}
	
	public Integer getNotificationType() {
		return notificationType;
	}
	public void setNotificationType(Integer notificationType) {
		this.notificationType = notificationType;
	}
	public String getPrevApprover() {
		return prevApprover;
	}
	public void setPrevApprover(String prevApprover) {
		this.prevApprover = prevApprover;
	}
	public String getNextApprover() {
		return nextApprover;
	}
	public void setNextApprover(String nextApprover) {
		this.nextApprover = nextApprover;
	}
	public String getRecipientEmailId() {
		return recipientEmailId;
	}
	public void setRecipientEmailId(String recipientEmailId) {
		this.recipientEmailId = recipientEmailId;
	}
	public String getSolutionCreator() {
		return solutionCreator;
	}
	public void setSolutionCreator(String solutionCreator) {
		this.solutionCreator = solutionCreator;
	}
	
}
