/*************************************************** MAIN MENU CSS START FROM HERE **********************************************/
/********************************************************************************************************************************/

.container {width: 100%; margin: 0 auto; margin-bottom:80px;margin-top:5px; font: 10px normal Verdana, Arial, Helvetica, sans-serif; }
ul#topnav {
	margin: 0; padding: 0;
	float: left;
	width: 1112px;
	list-style: none;
	position: relative;
	font-size: 13px;
	font-weight:bold;
	height:36px;
	-moz-border-radius-topleft:6px;
	-moz-border-radius-topright:6px;
	
	-webkit-border-top-left-radius:6px;
	-webkit-border-top-right-radius:6px;
	
	border-top-left-radius:6px;
	border-top-right-radius:6px;
	
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#0c3d67', endColorstr='#275d92'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#0c3d67), to(#275d92)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #0c3d67,  #275d92); /* for firefox 3.6+ */
	
}
ul#topnav li {
	float: left;
	width:158px;
	text-align:center;
	border-right:1px solid #356fa7;
}
ul#topnav li a {
	padding: 10px 10px;
	display: block;
	color: #FFC;
	border-right:1px solid #072e4f;
	text-decoration: none;
}
ul#topnav li a:hover {
	color:#602a00;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff7c1d', endColorstr='#f4c748'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#ff7c1d), to(#f4c748)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #ff7c1d,  #f4c748); /* for firefox 3.6+ */
}
ul#topnav li a.imghover {
	
	-moz-border-radius-topleft:6px;
	-webkit-border-top-left-radius:6px;
	border-top-left-radius:6px;
	
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#0c3d67', endColorstr='#275d92'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#0c3d67), to(#275d92)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #0c3d67,  #275d92); /* for firefox 3.6+ */
}
ul#topnav li a.rightborder {
	border:0px;
	-moz-border-radius-topright:5px;
	
	-webkit-border-top-right-radius:5px;
	
	border-top-right-radius:5px;
}
ul#topnav li.rightborder {
	border:0px;
}
ul#topnav li:hover {
	background-color:none;
	
}
ul#topnav li span {
	float: left;
	text-align:center;
	height:33px;
	line-height:32px;
	position: absolute;
	left: 0; top:35px;
	display: none;
	width: 100%;
	background: #e9e7f8;
	color: #292929;
	
	-moz-border-radius-bottomleft:6px;
	-moz-border-radius-bottomright:6px;
	
	-webkit-border-bottom-left-radius:6px;
	-webkit-border-bottom-right-radius:6px;
	
	border-bottom-left-radius:6px;
	border-bottom-right-radius:6px;
}
ul#topnav li:hover span { display: block;    }
ul#topnav li span a {
	display: inline;
	color: #6C4E38;
	border: none;
	padding: 5px;
	font-size: 12px;
	font-weight: bold;
	margin-right: 4px;
	margin-left: 4px;
	border-radius: 6px;
	background: none repeat scroll 0 0 #d3d4d7;
}

ul#topnav li span a:hover {
	padding: 11px;
	color: #fff;
	font-weight: bold;
	background: none repeat scroll 0 0 #BFA99D;
}

.MenuLeftRadius{
	-moz-border-radius-topleft:0px;
	-webkit-border-top-left-radius:0px;
	border-top-left-radius:0px;
}

/*************************************************** MAIN MENU CSS START FROM HERE **********************************************/
