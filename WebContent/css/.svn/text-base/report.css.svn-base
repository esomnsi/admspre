/*** CSS for reports *****/
.reportDiv table{
    border-collapse: collapse;
    width:100%;
    
}
.reportDiv table th{
    
    font-family: arial;
    font-size: 12px;
   	text-align:center;
    height:25px;
  	color: #000000;
	font-weight: bold;
	background-color: #efefef;
	border: 1px solid #D6D6D6;
}

