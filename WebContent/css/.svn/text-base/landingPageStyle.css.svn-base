.basicForm_wrapper{
	/* width:438px; height:335px; */ float:left; 
	width:465px; height:350px;
/*	background-color:#d1e3ed;*/
	padding:1px; 
	
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	float:left;
	}
.basicForm_headerPanel{
	width:100%;
	height:30px;
	float:left;
		}


.myActionButton{
	width:130px;
	height:28px;
	float:left;
	background-color:#fbfbfb;
	
	-moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    -khtml-border-radius: 6px;
    border-radius: 6px;
	
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#275c92', endColorstr='#244486'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#275c92), to(#244486)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #275c92,  #244486); /* for firefox 3.6+ */
	}

.myActionButton:hover{
	cursor:pointer;	
	color:#602a00;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f78a3b', endColorstr='#f4c748'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#f78a3b), to(#f4c748)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #f78a3b,  #f4c748); /* for firefox 3.6+ */
	}
	
.myActionButton h1{
	font-family:Arial;
	line-height:18px;
	font-size:12px;
	font-weight:normal;
	color:#ffffff;
	font-style:italic;
	font-weight:bold;
	text-align:left;
	float:left;
	padding:0px 15px 0px 20px;
	margin:4px 0 0 0;
	}
	
.myActionButton span{
	float:left;
	width:30px;
	height:28px;
	padding:0px;

	background:url(../images/nextPageIcon.png) left center no-repeat
	}
	
	
.basicForm_activePanel{
	width:100%;
	height:28px;
	float:left;
	margin-bottom:6px;
	background-color:#fbfbfb;
	
	-moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    -khtml-border-radius: 6px;
    border-radius: 6px;
	
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#275c92', endColorstr='#244486'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#275c92), to(#244486)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #275c92,  #244486); /* for firefox 3.6+ */
	}
	
.basicForm_activePanel h1{
	font-family:Arial;
	line-height:18px;
	font-size:12px;
	font-weight:normal;
	color:#ffffff;
	font-style:italic;
	font-weight:bold;
	text-align:left;
	float:left;
	padding:0px 15px 0px 20px;
	margin:4px 0 0 0;
	}
.basicForm_activePanel span{
	float:left;
	width:30px;
	height:28px;
	margin-right:20px;
	padding:0px;

	background:url(../images/nextPageIcon.png) left center no-repeat
	}
	
.basicForm_deActivePanel{
	width:227px;
	height:37px;
	float:left;
	}
.basicForm_deActivePanel h1{
	font-family:Arial;
	width:150px;
	font-size:12px;
	color:#7bb0cd;
	text-align:right;
	float:right;
	font-weight:bold;
	padding:0px;
	margin:4px 0 0 0;
	}
.basicForm_deActivePanel span{
	float:right;
	width:30px;
	height:37px;
	margin-right:20px;
	padding:0px;
	opacity:0.4;

	background:url(../images/nextPageIcon.png) right center no-repeat
	}
.basicForm_bodyPanel{
	width:335;
	height:346px;
	
	float:left;
	padding:1px;
	border:1px solid #8aa4db;
	background-color:#fbfbfb;
	
	-moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    -khtml-border-radius: 6px;
    border-radius: 6px;
	
	border-bottom-left-radius:6px;
	border-bottom-right-radius:6px;
	
	}
.informationPanel{
	width:100%;
	height:26px;
	float:left;
	}
.commentinformationPanel{
	width:100%;
	height:46px;
	float:left;
	}
.textHedrPanel{
	width:100px;
	margin-left:20px;
	height:22px;
	font-family:Arial;
