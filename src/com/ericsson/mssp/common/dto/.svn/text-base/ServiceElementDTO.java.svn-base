package com.ericsson.mssp.common.dto;


/**
 * @author egaivij
 *
 */
public class ServiceElementDTO extends BaseDTO {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private Integer serviceElementID;
	private String serviceFunctionName;
	private String serviceFunctionCode;
	private String serviceElementName;
	private String serviceElementDescription;
	private String serviceType;
	private String active;
	private String hasServiceScope;
	//private Set<ServiceScope> serviceScopes = new HashSet<ServiceScope>(0);
	
	
	public Integer getServiceElementID() {
		return serviceElementID;
	}
	public void setServiceElementID(Integer serviceElementID) {
		this.serviceElementID = serviceElementID;
	}
	public String getServiceFunctionName() {
		return serviceFunctionName;
	}
	public void setServiceFunctionName(String serviceFunctionName) {
		this.serviceFunctionName = serviceFunctionName;
	}
	public String getServiceFunctionCode() {
		return serviceFunctionCode;
	}
	public void setServiceFunctionCode(String serviceFunctionCode) {
		this.serviceFunctionCode = serviceFunctionCode;
	}
	public String getServiceElementName() {
		return serviceElementName;
	}
	public void setServiceElementName(String serviceElementName) {
		this.serviceElementName = serviceElementName;
	}
	public String getServiceElementDescription() {
		return serviceElementDescription;
	}
	public void setServiceElementDescription(String serviceElementDescription) {
		this.serviceElementDescription = serviceElementDescription;
	}
	public String getActive() {
		return active;
	}
	public void setActive(String active) {
		this.active = active;
	}
	public String getHasServiceScope() {
		return hasServiceScope;
	}
	public void setHasServiceScope(String hasServiceScope) {
		this.hasServiceScope = hasServiceScope;
	}
	/**
	 * @return the serviceType
	 */
	public String getServiceType() {
		return serviceType;
	}
	/**
	 * @param serviceType the serviceType to set
	 */
	public void setServiceType(String serviceType) {
		serviceType = serviceType;
	}
	
}
