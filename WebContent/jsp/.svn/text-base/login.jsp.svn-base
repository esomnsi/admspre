<%@taglib uri="http://www.springframework.org/tags/form" prefix="form"%>


<form:form action="%{pageContext.request.contextPath}/j_spring_security_check" method="post" modelAttribute="userDTO">
      <table>
          <tr>
              <td>Name:</td>
              <td><form:input path="j_username" type="text"/></td>
          </tr>
       
          <tr>
              <td colspan="2">
                  <input type="submit" value="Get Email" />
              </td>
          </tr>
          
             <tr>
              <td>Email</td>
              <td><form:input path="email" type="text"/></td>
          </tr>
      </table>
  </form:form>