body{
		font-family:Arial;
		margin-top:0px;
					
	}
	.bodyText{
		padding-left:18px;
		padding-right:10px;
		border-top:1px solid white;
		padding-left:18px;
			
	}
	.bodyText p{
		margin-top:5px;
	}
	#mainContainer{
		padding-left:8px;
		width:95%;
		height:900px;
		border:1px solid #000000;
		margin:0 auto;
		background-image:url('../../images/heading3.gif');
		background-repeat:no-repeat;
		padding-top:85px;	
        position:relative;
    	min-height:50px;
    	margin-left:70px;
   		
	}
	#mainMenu{
		padding-left:10px;		
		background-color: #FFF;	/* Background color of main menu */
		font-family:Arial;
		font-size:13px;	/* Font size of main menu items */
		border-bottom:0px solid #000000;	/* Bottom border of main menu */
		height:35px;	/* Height of main menu */
		border:0px 0px 0px 0px;
		position:relative;	/* Don't change this position attribute */
		font-weight:bold;
		margin-top:0px;
		margin-left:-17px;
		width:91%;	
		
		
	}
	#mainMenu a{
		width: 140px;
		padding-left:15px;	/* Spaces at the left of main menu items */
		padding-right:14px;	/* Spaces at the right of main menu items */
		font-weight:bold;
		padding-top:4px;
		font-family:Arial;
		text-align:center;
		height:25px;	
		/* Don't change these two options */
		position:absolute;
		bottom:-1px;	/* Change this value to -2px if you're not using a strict doctype */
		background-color:#002661;
		color:black;
		border-left:1px solid #000000;
		border-top:1px solid #000000;
		border-right:1px solid #000000; 
		
	}
	#submenu{		
	font-family:Arial;
		background-color:#A4A4A4;	/* Background color of sub menu items */
		padding-left: -10px;
		width:100%;	/* Don't change this option */
		height:25px;
		border-bottom:1px solid #000000;
		border-left:1px solid #000000;
		border-right:1px solid #000000;
		border-top:1px solid #000000;		
		margin-left:-2px;	
		
	}	
	#submenu div{
		white-space:nowrap;	/* Don't change this option */
		
	}
	/*
	Style attributes of active menu item 
	*/
	#mainMenu .activeMenuItem{
		/* Border options */
		border-left:1px solid #000000;
		border-top:1px solid #000000;
		border-right:1px solid #000000;	

		background-color: #848484;	/* Background color */
		font-weight:bold;
		cursor:pointer;	
	}
	
	#mainMenu .activeMenuItem img{
		position:absolute;
		bottom:0px;
		right:0px;
	}
		
	#mainMenu .inactiveMenuItem{		
		color: white;
		cursor:pointer;	
	}
	
	  #submenu a {	
		text-decoration:none;	
		padding-left:1px;	
		padding-right:1px;
		color: #000;	
		font-size:12px; 
		white-space:nowrap;
	} 
	
	
	.borderClass{
		-moz-border-radius: 4px 4px 0px 0px;
	
	}
	
	/* .linkClass{
		text-decoration:none;	
		padding-left:5px;	
		padding-right:5px;
		color: black;;	
		font-size:13px; 
	} */
	
	#submenu a:hover{
		color: black;/* Red color when the user moves the mouse over sub menu items */
		text-decoration:underline;	
		
	}

	ul#submenu ul li a .current{
		color: #00477D;
		text-decoration:underline;			
	}
	
	


ul#submenu ul li.subActive li a
{
    color:#00477D;
    text-decoration: none;
}





	/*************************/
	
/* links */
ul#tabmenu ul li li a
{
    line-height:22px;
    padding: 0; 
    white-space: nowrap;
    font-size:12px;
    font-weight:normal;
    font-family:Arial;
}
/* non-active links */
ul#tabmenu ul li.subActive li a
{
    color:#00477D;
    text-decoration: none;
}
/* active link */
ul#tabmenu ul li.subActive li.subActive a, ul#tabmenu ul li.subActive li a:hover
{
    color:#00477D;
    text-decoration: underline;
}

ul#tabmenu li.active li li
{
    display: block;
    *clear:both;/*IE hack*/
}

ul#tabmenu li.active ul ul
{	
    text-align:left;
    padding:4px 24px 10px 24px;
    left: 0px; /*offset*/
    width: auto;
    /*background:#00abd6;*/
     background:#848484
    border:1px solid #000000;
    border-top: 0;
    *top: 25px;
    display: none;
    position: absolute;
    z-index: 4;
    border-radius: 0 0 4px 4px;
}
ul#tabmenu li.active li
{
    position: relative;
}

#t_submenu{

	visibility: hidden;
}



