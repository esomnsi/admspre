package com.ericsson.mssp.approval.forms;

import java.util.List;

import com.ericsson.mssp.common.dto.OpportunityDTO;
import com.ericsson.mssp.common.dto.UserDTO;
import com.ericsson.mssp.common.entity.SolutionADR;

public class ApprovalDetails {
	
	private List<UserDTO> representatives;
	private String Level1Approver;
	private String Level2Approver;
	private String Level3Approver;
	private String comments;
	private int solutionId;
	private int solutionStatus;
	private boolean approverListChangeInitiated = false;
	private boolean allowAction=true;
	private CheckListElements checkListElements;
	
	
	public List<UserDTO> getRepresentatives() {
		return representatives;
	}
	public void setRepresentatives(List<UserDTO> representatives) {
		this.representatives = representatives;
	}
	public String getLevel1Approver() {
		return Level1Approver;
	}
	public void setLevel1Approver(String level1Approver) {
		Level1Approver = level1Approver;
	}
	public String getLevel2Approver() {
		return Level2Approver;
	}
	public void setLevel2Approver(String level2Approver) {
		Level2Approver = level2Approver;
	}
	public String getLevel3Approver() {
		return Level3Approver;
	}
	public void setLevel3Approver(String level3Approver) {
		Level3Approver = level3Approver;
	}
	public int getSolutionId() {
		return solutionId;
	}
	public void setSolutionId(int solutionId) {
		this.solutionId = solutionId;
	}
	public int getSolutionStatus() {
		return solutionStatus;
	}
	public void setSolutionStatus(int solutionStatus) {
		this.solutionStatus = solutionStatus;
	}
	
	/**
	 * @return the approverListChangeInitiated
	 */
	public boolean isApproverListChangeInitiated() {
		return approverListChangeInitiated;
	}
	/**
	 * @param approverListChangeInitiated the approverListChangeInitiated to set
	 */
	public void setApproverListChangeInitiated(boolean approverListChangeInitiated) {
		this.approverListChangeInitiated = approverListChangeInitiated;
	}
	
	public boolean isAllowAction() {
		return allowAction;
	}
	public void setAllowAction(boolean allowAction) {
		this.allowAction = allowAction;
	}
	
	public CheckListElements getCheckListElements() {
		return checkListElements;
	}
	public void setCheckListElements(CheckListElements checkListElements) {
		this.checkListElements = checkListElements;
	}
	public String getComments() {
		return comments;
	}
	public void setComments(String comments) {
		this.comments = comments;
	}
	
}
